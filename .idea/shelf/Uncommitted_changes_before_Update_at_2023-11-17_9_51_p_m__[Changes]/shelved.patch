+package com.axlr8.backend.Model;
+
+import java.util.UUID;
+//import com.fasterxml.jackson.annotation.JsonBackReference;
+import com.fasterxml.jackson.annotation.JsonManagedReference;
+import jakarta.persistence.CascadeType;
+import jakarta.persistence.Entity;
+import jakarta.persistence.GeneratedValue;
+import jakarta.persistence.GenerationType;
+import jakarta.persistence.Id;
+import jakarta.persistence.JoinColumn;
+import jakarta.persistence.OneToOne;
+import jakarta.persistence.Table;
+
+@Entity
+@Table
+public class VisitEvent {
+    @Id
+    @GeneratedValue(strategy = GenerationType.UUID)
+    private UUID id;
+
+    private String ipaddress; // 20 characters
+    private String day; // Format is DD/MM/YYYY, 8 characters
+    private String vid; // 20 characters, vehicle id
+    private String eventType; // Website status (VIEW, CART, PURCHASE)
+
+    @OneToOne(cascade = CascadeType.ALL)
+    @JoinColumn(name = "cartItem_id")
+    @JsonManagedReference(value = "cartItem-visitEvent")
+    private CartItem cartItem;
+
+    public VisitEvent() {
+    }
+
+    public VisitEvent(UUID id, String ipaddress, String day, String vid, String eventType) {
+        this.id = id;
+        this.ipaddress = ipaddress;
+        this.day = day;
+        this.vid = vid;
+        this.eventType = eventType;
+    }
+
+    public VisitEvent(String ipaddress, String day, String vid, String eventType, CartItem cartItem) {
+        this.ipaddress = ipaddress;
+        this.day = day;
+        this.vid = vid;
+        this.eventType = eventType;
+        this.cartItem = cartItem;
+    }
+
+    public UUID getId() {
+        return id;
+    }
+
+    public void setId(UUID id) {
+        this.id = id;
+    }
+
+    public String getIpaddress() {
+        return ipaddress;
+    }
+
+    public void setIpaddress(String ipaddress) {
+        this.ipaddress = ipaddress;
+    }
+
+    public String getDay() {
+        return day;
+    }
+
+    public void setDay(String day) {
+        this.day = day;
+    }
+
+    public String getVid() {
+        return vid;
+    }
+
+    public void setVid(String vid) {
+        this.vid = vid;
+    }
+
+    public String getEventType() {
+        return eventType;
+    }
+
+    public void setEventType(String eventType) {
+        this.eventType = eventType;
+    }
+
+    public CartItem getCartItem() {
+        return cartItem;
+    }
+
+    public void setCartItem(CartItem cartItem) {
+        this.cartItem = cartItem;
+    }
+}
